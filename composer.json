{
  "name": "berthott/laravel-crudable",
  "description": "Laravel Helper for CRUD Resources",
  "license": "MIT",
  "authors": [
      {
          "name": "Jan Bladt",
          "email": "berthott@web.de"
      }
  ],
  "autoload" : {
      "psr-4" : {
          "berthott\\Crudable\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "berthott\\Crudable\\Tests\\": "tests"
      }
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "berthott\\Crudable\\CrudableServiceProvider"
          ]
      }
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5",
    "doctrine/dbal": "^3.3"
  },
  "require": {
    "berthott/laravel-scopeable": "@dev",
    "haydenpierce/class-finder": "^0.4.3",
    "spatie/laravel-query-builder": "^5.0"
  },
  "repositories": [
      {
          "type": "path",
          "url": "../../laravel_packages/laravel-scopeable",
          "options": {
              "symlink": true
          }
      }
  ]
}
